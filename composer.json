{
  "name": "price-cli/read-cli",
  "description": "Read CLI Script",
  "type": "project",
  "require": {
    "php": "^8.1",
    "monolog/monolog": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "friendsofphp/php-cs-fixer": "^3.22"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "php-cs-fixer": "php-cs-fixer fix",
    "tests": "phpunit tests"
  }
}
